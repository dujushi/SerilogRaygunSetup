{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Information",
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\serilog-raygun-setup.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Raygun",
        "Args": {
          "applicationKey": "[ApplicationKey]",
          "tags": [ "Tag 1", "Tag 2" ]
        }
      }
    ]
  }
}